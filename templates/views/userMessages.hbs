<!doctype html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
<script src='https://kit.fontawesome.com/a076d05399.js' crossorigin='anonymous'></script>
    <title>contact us</title>
    <style>

    
      .container-fluid{
     background-color: black;
     height: 100vh;
     color:white;
      }
      body{

            align-items: center;
  
    padding: 0px 50px;


   
      }
      .card-body{


         box-shadow: 2px 2px 1px solid rgba(0, 0, 0, 0.865);
      }
    </style>
  </head>
  <body style="background-color:#f1f2f4;">








{{!-- cards body --}}




{{#each us}}


<div class="alert alert-success alert-dismissible fade show" role="alert">
<div class="container d-flex justify-content-center align-item-center mt-5">

  <div class="card w-50 d-flex"><small class="ml-auto text-danger">{{this.userDate}}</small>
  <div class="card-body">
    <h5 class="card-title text-info">User Details</h5>
    <p class="card-text"><strong>User name </strong> {{this.userName}}</p>
        <p class="card-text"><strong>User email </strong>  {{this.userEmail}}</p>
            
               <p class="card-text"><strong>User Message</strong>  {{this.userMessage}}</p>

 

</div>

</div>

</div>
  <button type="button" class="close " data-dismiss="alert" aria-label="Close" style="width:1% ;">
    <span aria-hidden="true">&times;</span>
  </button>
</div>


{{/each}}








</body>



    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
